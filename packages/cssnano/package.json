{
  "name": "cssnano",
  "version": "4.0.0-rc.2",
  "description": "A modular minifier, built on top of the PostCSS ecosystem.",
  "main": "dist/index.js",
  "scripts": {
    "bundle-size": "webpack --json --config src/__tests__/_webpack.config.js | webpack-bundle-size-analyzer",
    "integrations": "babel-node src/__tests__/util/rebuild.js",
    "prepublish": "cross-env BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/"
  },
  "keywords": [
    "css",
    "compress",
    "minify",
    "optimise",
    "optimisation",
    "postcss",
    "postcss-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "cosmiconfig": "^2.0.0",
    "cssnano-preset-default": "^4.0.0-rc.2",
    "is-resolvable": "^1.0.0",
    "postcss": "^6.0.0"
  },
  "devDependencies": {
    "array-to-sentence": "^1.0.0",
    "babel-cli": "^6.0.0",
    "babel-loader": "^6.0.0",
    "cross-env": "^3.0.0",
    "cssnano-preset-advanced": "^4.0.0-rc.2",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "postcss-font-magician": "^2.0.0",
    "webpack": "^1.0.0",
    "webpack-bundle-size-analyzer": "^2.0.0"
  },
  "homepage": "https://github.com/ben-eb/cssnano",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/cssnano",
  "files": [
    "dist",
    "LICENSE-MIT",
    "quickstart.js"
  ],
  "tonicExampleFilename": "quickstart.js",
  "bugs": {
    "url": "https://github.com/ben-eb/cssnano/issues"
  },
  "engines": {
    "node": ">=4"
  }
}
