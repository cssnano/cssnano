{
  "private": true,
  "name": "cssnano",
  "homepage": "http://cssnano.co",
  "scripts": {
    "lint:js": "eslint .",
    "lint:css": "stylelint \"web_modules/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "node scripts/createChangelog && node scripts/createOptimisations && phenomic build",
    "test": "npm run lint"
  },
  "phenomic": {
    "CNAME": true
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "cssnano",
      "plugin:react/recommended",
      "plugin:import-order/recommended"
    ],
    "rules": {
      "object-curly-spacing": 0,
      "babel/object-curly-spacing": 1,
      "react/display-name": 0,
      "react/no-danger": 0
    },
    "parser": "babel-eslint",
    "plugins": [
      "import-order",
      "react",
      "babel"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "selector-pseudo-element-colon-notation": "single"
    }
  },
  "devDependencies": {
    "@beneb/remark-behead": "^2.0.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-webpack-loaders": "^0.3.9",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "camelcase": "^2.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.0.0",
    "eslint-config-cssnano": "^2.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import-order": "^2.1.2",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "history": "^2.0.0",
    "html-loader": "^0.4.3",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "markdown-loader": "^0.1.7",
    "mini-unique-id": "^1.0.1",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.17.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.7.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "react-dom": "^0.14.0 || ^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-lowlight": "^1.0.0",
    "react-midas": "^1.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.2",
    "react-sticky": "^5.0.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "reactable": "^0.13.0",
    "redux": "^3.0.0",
    "remark": "^6.0.0",
    "remark-autolink-headings": "^4.0.0",
    "remark-github": "^6.0.0",
    "remark-highlight.js": "^4.0.0",
    "remark-html": "^5.0.0",
    "remark-react": "^3.0.0",
    "remark-slug": "^4.1.0",
    "style-loader": "^0.12.3",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "toml-loader": "^1.0.0",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^0.11.0"
  }
}
